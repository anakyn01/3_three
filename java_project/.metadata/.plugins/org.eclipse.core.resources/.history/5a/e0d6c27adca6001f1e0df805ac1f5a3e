package com.cocea.lesson.object;

import java.util.HashMap;

public class Hash {

	public static void main(String[] args) {
		//앞전에서 했던것처럼 스트링을 추가할때 문제가 생길경우 그럴땐 key / value
		HashMap<String, String> 수도 = new HashMap<String, String>();
		수도.put("한국", "서울");
		수도.put("독일", "베를린");
		수도.put("미국", "DC");
	}

}
